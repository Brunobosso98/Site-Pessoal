{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/openai/:path*",
      "destination": "https://api.openai.com/:path*"
    }
  ],
  "routes": [
    {
      "src": "/api/openai",
      "dest": "/api/openai.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}
